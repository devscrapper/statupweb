<p id="notice"><%= notice %></p>

<h1>Listing Ranks</h1>

<table>
  <thead>
    <tr>
      <th>Statistic type</th>
      <th>Monday start</th>
      <th>Count weeks</th>
      <th>Keywords</th>
      <th>Count visits per day</th>
      <th>Website</th>
      <th>Statistic</th>
      <th>Min count page organic</th>
      <th>Max count page organic</th>
      <th>Min duration page organic</th>
      <th>Max duration page organic</th>
      <th>Min duration</th>
      <th>Max duration</th>
      <th>Min duration website</th>
      <th>Min pages website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ranks.each do |rank| %>
      <tr>
        <td><%= rank.statistic_type %></td>
        <td><%= rank.monday_start %></td>
        <td><%= rank.count_weeks %></td>
        <td><%= rank.keywords %></td>
        <td><%= rank.count_visits_per_day %></td>
        <td><%= rank.website %></td>
        <td><%= rank.statistic %></td>
        <td><%= rank.min_count_page_organic %></td>
        <td><%= rank.max_count_page_organic %></td>
        <td><%= rank.min_duration_page_organic %></td>
        <td><%= rank.max_duration_page_organic %></td>
        <td><%= rank.min_duration %></td>
        <td><%= rank.max_duration %></td>
        <td><%= rank.min_duration_website %></td>
        <td><%= rank.min_pages_website %></td>
        <td><%= link_to 'Show', rank %></td>
        <td><%= link_to 'Edit', edit_rank_path(rank) %></td>
        <td><%= link_to 'Destroy', rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rank', new_rank_path %>
