<div class="container-fluid">


  <div id="visits<%= policy_id %>" style="">

    <%= render partial: 'visits/visits', :locals => {:execution_mode => execution_mode,
                                                     :policy_id => policy_id,
                                                     :policy_type => policy_type,
                                                     :state => state,
                                                     visits: Visit.where({:policy_id => policy_id,
                                                                          :policy_type => policy_type,
                                                                          :state => state}).order("plan_time desc")} %>
  </div>

</div>