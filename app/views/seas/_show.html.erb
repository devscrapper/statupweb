<div class="container-fluid">
  <div class="panel panel-default">

    <div class="panel-body">

      <div class="form-group">
        <%= label_tag :label %>
        <div class="col-lg-3">
          <%= text_field_tag :label, nil, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= label_tag :advertiser %>
        <div class="col-lg-3">
          <%= text_field_tag :advertiser, nil, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= label_tag :keywords %>
        <div class="col-lg-3">
          <%= text_field_tag :keywords, nil, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">

        <%= label_tag :label_advertisings %>
        <div class="col-lg-3">
          <%= text_field_tag :label_advertisings, nil, class: "form-control" %>
        </div>
      </div>


    </div>

  </div>
</div>


