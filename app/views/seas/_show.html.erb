<div class="container-fluid">

  <form class="form-horizontal" role="form">
    <div class="form-group">
      <%= label_tag :label, nil, class: "col-sm-2 control-label" %>
      <div class="col-sm-10">
        <%= text_field_tag  nil, sea.label , class: "form-control", disabled: true %>
      </div>
    </div>
    <div class="form-group">
      <%= label_tag :advertiser, nil, class: "col-sm-2 control-label" %>
      <div class="col-sm-10">
        <%= text_field_tag nil, sea.advertiser, class: "form-control", disabled: true %>
      </div>
    </div>
    <div class="form-group">
      <%= label_tag :keywords, nil, class: "col-sm-2 control-label" %>
      <span class="badge"> <%= sea.keywords.size %></span>
      <div class="col-sm-10">
        <%= text_area_tag nil, sea.keywords_dspl, class: "form-control", disabled: true, rows: 5, cols: 25 %>
      </div>
    </div>
    <div class="form-group">
      <%= label_tag :label_advertisings, nil, class: "col-sm-2 control-label" %>
      <span class="badge"> <%= sea.label_advertisings.size  %></span>
      <div class="col-sm-10">
        <%= text_area_tag nil, sea.advertisings_dspl, class: "form-control", disabled: true, rows: 5, cols: 25 %>
      </div>
    </div>
  </form>


</div>


