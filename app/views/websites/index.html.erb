<p id="notice"><%= notice %></p>

<h1>Listing Websites</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Profil id ga</th>
      <th>Url root</th>
      <th>Count page</th>
      <th>Schemes</th>
      <th>Types</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @websites.each do |website| %>
      <tr>
        <td><%= website.label %></td>
        <td><%= website.profil_id_ga %></td>
        <td><%= website.url_root %></td>
        <td><%= website.count_page %></td>
        <td><%= website.schemes %></td>
        <td><%= website.types %></td>
        <td><%= link_to 'Show', website %></td>
        <td><%= link_to 'Edit', edit_website_path(website) %></td>
        <td><%= link_to 'Destroy', website, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Website', new_website_path %>
